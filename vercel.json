{
    "outputDirectory": "client/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/resume.pdf",
            "destination": "/resume.pdf"
        },
        {
            "source": "/((?!api/|.*\\..+).*)",
            "destination": "/index.html"
        }
    ]
}